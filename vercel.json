{
  "version": 2,
  "functions": {
    "api/send-email.js": {
      "maxDuration": 30
    },
    "api/test.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
